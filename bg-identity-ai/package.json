{
  "name": "bg-identity-ai",
  "version": "1.0.0",
  "description": "BehaviorGuard Identity AI Service - Biometric verification and document processing",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
